{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    },
    {
      "src": "bot.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/link",
      "dest": "app.py"
    },
    {
      "src": "/bot",
      "dest": "bot.py"
    }
  ]
}
